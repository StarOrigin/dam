<Window x:Class="org.starorigin.dam.launcher.PMWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ViewModels="clr-namespace:org.starorigin.dam.launcher.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:dxdo="http://schemas.devexpress.com/winfx/2008/xaml/docking"
        xmlns:local="clr-namespace:org.starorigin.dam.launcher"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="PMWindow" Width="1280" Height="800"
        d:DesignHeight="1024" d:DesignWidth="768" mc:Ignorable="d">
    <Grid>
        <dxdo:DockLayoutManager>
            <dxdo:LayoutGroup>
                <dxdo:LayoutPanel Caption="照片管理">
                    <ViewModels:PhotoView />
                </dxdo:LayoutPanel>
            </dxdo:LayoutGroup>
        </dxdo:DockLayoutManager>
    </Grid>
</Window>
